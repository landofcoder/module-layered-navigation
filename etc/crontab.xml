<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="loffilterstock">
        <job name="lofupdatestockfilter" instance="Lof\LayeredNavigation\Cron\UpdateStockFilter" method="execute">
          <config_path>layered_navigation/cronjobs/cronjobupdate</config_path>
        </job>
        <job name="lofsetstockfilter" instance="Lof\LayeredNavigation\Cron\SetStockFilter" method="execute">
          <config_path>layered_navigation/cronjobs/cronjobset</config_path>
        </job>
    </group>
</config>
